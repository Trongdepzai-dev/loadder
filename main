local Window = nil

local function CreateGUI()
    -- Destroy window cÅ© náº¿u cÃ³
    if Window then
        pcall(function()
            Rayfield:Destroy()
        end)
        wait(0.5)
        -- Reload Rayfield
        Rayfield = loadstring(game:HttpGet('https://sirius.menu/rayfield'))()
    end
    
