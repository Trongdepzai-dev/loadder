    -- ğŸ“Œ NÃšT LOAD SCRIPT Tá»° Äá»˜NG
    -- â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•
    
    MainTab:CreateButton({
        Name = L("btn_auto_load"),
        Callback = function()
            if GameInfo then
                Rayfield:Notify({
                    Title = L("notify_loading"),
                    Content = L("notify_loading_content") .. GameInfo.Name,
                    Duration = 3,
                    Image = 4483362458
                })
                
                wait(1)
                
                local success, err = pcall(function()
                    loadstring(game:HttpGet(GameInfo.Script, true))()
                end)
                
                if success then
                    Rayfield:Notify({
                        Title = L("notify_success"),
                        Content = GameInfo.Name .. L("notify_success_content"),
                        Duration = 5,
                        Image = 4483362458
                    })
                else
                    Rayfield:Notify({
                        Title = L("notify_error"),
                        Content = L("notify_error_content") .. tostring(err),
                        Duration = 5,
                        Image = 4483362458
                    })
                end
            else
                Rayfield:Notify({
                    Title = L("notify_warning"),
                    Content = L("notify_warning_content"),
                    Duration = 3,
                    Image = 4483362458
                })
            end
        end
    })
    
