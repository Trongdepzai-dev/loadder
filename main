    local SettingsTab = Window:CreateTab(L("tab_settings"), 4483362458)
    
    -- â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•
    -- ğŸ“Œ PHáº¦N CHá»ŒN NGÃ”N NGá»®
    -- â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•
    
    SettingsTab:CreateSection(L("section_language"))
    
    SettingsTab:CreateDropdown({
        Name = L("dropdown_language"),
        Options = {"English", "Tiáº¿ng Viá»‡t"},
        CurrentOption = {CurrentLanguage == "en" and "English" or "Tiáº¿ng Viá»‡t"},
        MultipleOptions = false,
        Flag = "LanguageDropdown",
        Callback = function(Options)
            local selected = Options[1]
            local newLang = nil
            
            if selected == "English" then
                newLang = "en"
            elseif selected == "Tiáº¿ng Viá»‡t" then
                newLang = "vi"
            end
            
            if newLang and newLang ~= CurrentLanguage then
                CurrentLanguage = newLang
                
                Rayfield:Notify({
                    Title = L("notify_lang_changed"),
                    Content = L("notify_lang_content"),
                    Duration = 3,
                    Image = 4483362458
                })
                
                -- Reload GUI vá»›i ngÃ´n ngá»¯ má»›i
                wait(1)
                CreateGUI()
            end
        end
    })
    
    SettingsTab:CreateSection(L("section_customize"))
    
    SettingsTab:CreateButton({
        Name = L("btn_close"),
        Callback = function()
            Rayfield:Destroy()
        end
    })
    
    SettingsTab:CreateSection(L("section_info"))
    
    SettingsTab:CreateParagraph({
        Title = L("credits_title"),
        Content = L("credits_content")
    })
    
